[32m+[m[32mpublic class MyWorld extends World[m
[32m+[m[32m{[m
[32m+[m[32m    private boolean isPlaying = false;[m
[32m+[m[32m    Board board = new Board();[m
[32m+[m[32m    public GreenfootSound Grasswalk = new GreenfootSound("Grasswalk.wav");[m
[32m+[m[32m    public Zombie[][] level1 = {[m
[32m+[m[32m                {new BasicZombie(), null, new BasicZombie(), new BasicZombie(), new BasicZombie()},[m[41m [m
[32m+[m[32m                {new BasicZombie(), new BasicZombie(), new BasicZombie()}[m
[32m+[m[32m    };[m
[32m+[m[32m    public WaveManager level = new WaveManager(30000L, level1, 5000L);[m
[32m+[m[41m    [m
[32m+[m[32m    /**[m
[32m+[m[32m     * Constructor for objects of class MyWorld.[m
[32m+[m[32m     *[m[41m [m
[32m+[m[32m     */[m
[32m+[m[32m    public MyWorld()[m
[32m+[m[32m    {[m[41m    [m
[32m+[m[32m        // Create a new world with 600x400 cells with a cell size of 1x1 pixels.[m
[32m+[m[32m        super(840, 480, 1);[m[41m [m
[32m+[m[32m        Greenfoot.setSpeed(50);[m
[32m+[m[32m        setBackground("lawn2.5.png");[m
[32m+[m[41m       [m
[32m+[m[32m        addObject(board,0,0);[m
[32m+[m[41m        [m
[32m+[m[41m     [m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public void act() {[m
[32m+[m[32m        if (!isPlaying) {[m
[32m+[m[32m            Grasswalk.playLoop();[m
[32m+[m[41m            [m
[32m+[m[32m            isPlaying = true;[m
[32m+[m[41m            [m
[32m+[m[32m            for (int k = 0; k < board.Board[0].length; k++) {[m
[32m+[m[32m                board.placePlant(0,k, new Sunflower());[m
[32m+[m[32m            }[m
[32m+[m[32m            for (int k = 0; k < board.Board[0].length; k++) {[m
[32m+[m[32m                board.placePlant(1,k, new Peashooter());[m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[41m            [m
[32m+[m[41m            [m
[32m+[m[32m            addObject(level,0,0);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[32m}[m
