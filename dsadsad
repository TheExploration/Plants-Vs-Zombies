[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m    public void placePlant(int x, int y, Plant plant) {[m
[32m+[m[32m        if (Board[x][y] == null) {[m
[32m+[m[32m            Board[x][y] = plant;[m[41m    [m
[32m+[m[32m            World MyWorld = getWorld();[m
[32m+[m[32m            Plant temp = Board[x][y];[m
[32m+[m[32m            MyWorld.addObject(temp, x*xSpacing+xOffset, y*ySpacing+yOffset);[m
[32m+[m[32m            AudioPlayer.play(80, "plant.wav", "plant2.wav");[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public void updateBoard() {[m
[32m+[m[32m        for (int i = 0; i < Board.length; i++) {[m
[32m+[m[32m            for (int k = 0; k < Board[0].length; k++) {[m
[32m+[m[32m                if (Board[i][k] != null) {[m
[32m+[m[32m                    World MyWorld = getWorld();[m
[32m+[m[32m                    Plant temp = Board[i][k];[m
[32m+[m[32m                    MyWorld.addObject(temp, i*xSpacing+xOffset, k*ySpacing+yOffset);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
